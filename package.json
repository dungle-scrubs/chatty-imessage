{
  "name": "chat-imessage",
  "version": "1.0.0",
  "description": "CLI for reading and sending iMessages on macOS",
  "module": "src/index.ts",
  "type": "module",
  "bin": {
    "chat": "src/index.ts"
  },
  "files": [
    "src",
    "scripts"
  ],
  "scripts": {
    "start": "bun run src/index.ts",
    "chat": "bun run src/index.ts",
    "test": "bun test",
    "lint": "biome lint src/",
    "format": "biome format --write src/",
    "check": "biome check src/",
    "typecheck": "tsc --noEmit",
    "build": "bun build --compile --minify src/index.ts --outfile dist/chat",
    "install:local": "bun run build && ./scripts/install.sh",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dungle-scrubs/chat-imessage.git"
  },
  "keywords": [
    "imessage",
    "macos",
    "cli",
    "messages",
    "sms"
  ],
  "author": "Kevin Frilot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dungle-scrubs/chat-imessage/issues"
  },
  "homepage": "https://github.com/dungle-scrubs/chat-imessage#readme",
  "os": [
    "darwin"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest",
    "husky": "^9.1.7"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "date-fns": "^4.1.0"
  }
}
